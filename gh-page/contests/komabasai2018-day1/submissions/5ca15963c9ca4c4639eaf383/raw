global _start
p:mov eax,0
.A:inc cx
movzx bx,byte[ecx-1]
cmp bx,10
je .B
mov dx,10
mul edx
lea ax,[eax+ebx-48]
jmp .A
.B:ret
f:mov eax,0
.A:cmp ax,si
je .B
mov byte[ecx+eax],bl
inc ax
jmp .A
.B:mov byte[ecx+esi],10
ret
_start:sub esp,198
mov eax,3
mov ebx,0
lea ecx,[esp]
mov edx,9
int 80h
call p
mov edi,eax
call p
mov esi,eax
lea ecx,[esp+99]
mov bx,42
call f
sub ecx,99
mov bx,32
call f
mov byte[ecx],42
mov byte[ecx+esi-1],42
mov eax,4
mov ebx,1
lea ecx,[esp+99]
lea edx,[esi+1]
int 80h
sub ecx,99
sub edi,2
.I:mov eax,4
int 80h
dec edi
jne .I
mov eax,4
